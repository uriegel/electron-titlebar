{
  "name": "web-electron-titlebar",
  "version": "1.3.0",
  "description": "A html/javascript titlebar web component for electron without native window frame",
  "main": "src/index.js",
  "module": "src/index.js",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "typescript": "tsc -p typescript",
    "typescript-test": "tsc -p electron",
    "test": "electron ./electron/main.js",
    "do publish test": "npm run typescript && npm publish --dry-run",
    "do publish": "npm run typescript && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uriegel/electron-titlebar.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "web-components",
    "electron"
  ],
  "author": "Uwe Riegel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uriegel/electron-titlebar/issues"
  },
  "homepage": "https://github.com/uriegel/electron-titlebar#readme",
  "devDependencies": {
    "electron": "^15.2.0",
    "typescript": "^4.5.5"
  }
}
